<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <lang.foldingBuilder
            language="kotlin"
            implementationClass="org.polyfrost.intelliprocessor.editor.folding.PreprocessorFoldingKotlin"
            order="first"
        />

        <completion.contributor
            language="kotlin"
            implementationClass="org.polyfrost.intelliprocessor.editor.PreprocessorKeywordCompletion"
        />
    </extensions>
</idea-plugin>
